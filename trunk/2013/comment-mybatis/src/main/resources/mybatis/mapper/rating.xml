<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ac.jejunu.repository.mapper.RatingMapper">
	<insert id="addRating" parameterType="kr.ac.jejunu.model.Rating">
		INSERT INTO rating (commentId, userId)
		VALUES (#{commentId}, #{userId})
	</insert>
</mapper>