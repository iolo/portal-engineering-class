<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.bong.replyboard.repository.RatingDao">

	<insert id="insertUpRating" parameterType="kr.bong.replyboard.model.Rating">
		INSERT INTO upLog (replyNo, id, regiDate)
		VALUES (#{replyNo}, #{id}, now())
	</insert>
	
	<insert id="insertDownRating" parameterType="kr.bong.replyboard.model.Rating">
		INSERT INTO downLog (replyNo, id, regiDate)
		VALUES (#{replyNo}, #{id}, now())
	</insert>
	
</mapper>